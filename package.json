{
  "version": "0.7.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/growthbook/growthbook-js"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn esmdev",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "esmdev": "terser dist/growthbook.esm.js -c -m -o dist/growthbook.esm.min.js --source-map \"content='dist/growthbook.esm.js.map',url='growthbook.esm.min.js.map'\""
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@growthbook/growthbook",
  "author": "Jeremy Dorn",
  "module": "dist/growthbook-js.esm.js",
  "size-limit": [
    {
      "path": "dist/growthbook.cjs.production.min.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/growthbook.esm.min.js",
      "limit": "5 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "husky": "^4.3.8",
    "size-limit": "^4.9.2",
    "terser": "^5.6.0-beta",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dom-mutator": "^0.3.1"
  }
}
